{
  "name": "uncontrollable-fork",
  "version": "4.0.3",
  "description": "Wrap a controlled react component, to allow specific prop/handler pairs to be uncontrolled",
  "author": {
    "name": "Jason Quense",
    "email": "monastic.panic@gmail.com"
  },
  "repository": "jquense/uncontrollable",
  "license": "MIT",
  "main": "lib/index.js",
  "keywords": [
    "uncontrolled-component",
    "react-component",
    "input",
    "controlled",
    "uncontrolled",
    "form"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rimraf lib",
    "copy": "cpy ./README.md ./lib",
    "tdd": "jest --watch",
    "lint": "eslint src test",
    "build": "npm run test && npm run clean && babel src --out-dir lib && npm run copy",
    "release": "release"
  },
  "peerDependencies": {
    "react": ">=0.11.0"
  },
  "release-script": {
    "altPkgRootFolder": "lib"
  },
  "jest": {
    "rootDir": "./test"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-jest": "^19.0.0",
    "babel-preset-jason": "^2.0.0",
    "chai": "^3.5.0",
    "cpy": "^3.4.0",
    "dirty-chai": "^1.2.2",
    "eslint": "^3.19.0",
    "eslint-config-jason": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "imports-loader": "^0.6.3",
    "jest": "^19.0.2",
    "prop-types": "^15.5.6",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "release-script": "^1.0.2",
    "rimraf": "^2.2.8",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.9.0",
    "style-loader": "^0.8.3",
    "teaspoon": "^6.5.0",
    "webpack": "^1.4.15",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "invariant": "^2.1.0"
  }
}